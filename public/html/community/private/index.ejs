            <div id="board-tab-list" class="homepage_board">
              <div class="board_tab">
                <ul>
                  <li data-target="user">
                    <a href="#user-tab">회원</a>
                  </li>
                  <li data-target="message">
                    <a href="#message-tab">커뮤니티 메세지</a>
                  </li>
                  <li data-target="notice">
                    <a href="#notice-tab">프라이빗 게시판</a>
                  </li>
                </ul>
              </div>
              <div id="user-tab" class="board_con board_user active">
                <div class="title">
                  <div class="board_name">
                    <img src="/assets/board_user.png" alt="회원아이콘" />
                    <h3>회원</h3>
                    <p>(22)</p>
                  </div>
                  <div class="board_btn">
                    <button title="회원초대하기" class="invite_btn">
                      <!-- <img src="/assets/user_mail.png" alt="회원초대하기" /> -->
                      회원초대
                    </button>
                    <button title="목록보기" class="list_btn">
                      <img src="/assets/list_icon_01.png" alt="목록보기" />
                    </button>
                  </div>
                </div>
                <div class="board_table">
                  <table id="user-table" cellspacing="0">
                    <thead>
                      <tr>
                        <th width="8%">No.</th>
                        <th width="13%">닉네임</th>
                        <th width="5%">권한</th>
                        <th width="18%">전화번호</th>
                        <th width="22%">이메일</th>
                        <th width="13%">가입일</th>
                        <th width="13%">최종접속일</th>
                        <th width="8%"></th>
                      </tr>
                    </thead>
                  </table>
                </div>
              </div>

              <div id="message-tab" class="board_con board_message">
                <div class="title">
                  <div class="board_name">
                    <img src="/assets/board_message.png" alt="메세지아이콘" />
                    <h3>메세지</h3>
                    <!-- <span>NEW (3)</span> -->
                  </div>
                  <div class="board_btn">
                    <button title="메세지보내기" class="message_btn">
                      <!-- <img src="/assets/message_icon.png" alt="메세지보내기" /> -->
                      메세지발송
                    </button>
                    <button title="목록보기" class="list_btn" onclick="location.href = '/community/message/list/<%=communityId%>'">
                      <img src="/assets/list_icon_01.png" alt="목록보기" >
                    </button>
                  </div>
                </div>
                <div class="board_table">
                  <table id="message-table" cellspacing="0">
                    <thead>
                      <tr>
                        <th width="8%">No.</th>
                        <th width="65%">제목</th>
                        <th width="12%">작성자</th>
                        <th width="15%">
                          <span class="custom_sort">날짜</span>
                        </th>
                      </tr>
                    </thead>
                  </table>
                </div>
              </div>

              <div id="notice-tab" class="board_con board_notice">
                <div class="title">
                  <div class="board_name">
                    <img src="/assets/board_notice.png" alt="게시판아이콘" />
                    <h3>프라이빗 게시판</h3>
                  </div>
                  <div class="board_btn">
                    <button title="목록보기" class="list_btn" onclick="location.href = '/community/private/board/list/<%=communityId%>'">
                      <img src="/assets/list_icon_01.png" alt="목록보기" />
                    </button>
                  </div>
                </div>
                <div class="board_category">
                  <ul class="category_area">
                    <!-- <li>
                      <p>공지사항</p>
